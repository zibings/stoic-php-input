{
	"name": "stoic/io",
	"description": "Input and Output components for the Stoic framework",
	"version": "1.0.3",
	"homepage": "https://stoic-framework.com",
	"autoload": {
		"psr-0": {
			"Stoic\\Utilities\\Sanitizers\\": "./Utilities/Sanitizers",
			"Stoic\\Utilities\\": "./Utilities"
		},
		"psr-4": {
			"Stoic\\Utilities\\Sanitizers\\": "./Utilities/Sanitizers",
			"Stoic\\Utilities\\": "./Utilities"
		},
		"exclude-from-classmap": [
			"./Tests/"
		]
	},
	"require": {
		"php": "^7.0",
		"stoic/stoic": "^1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^7"
	}
}
